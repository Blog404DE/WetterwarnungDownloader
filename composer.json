{
    "name": "blog404de/wetterwarnungdownloader",
    "description": "Script zum herunterladen und auswerten der vom DWD bereitgestellten Warnungen vor Wettergefahren",
    "keywords": [
        "DWD",
        "Wettergefahren",
        "Wetterwarnung",
        "Unwetterwarnung"
    ],
    "type": "project",
    "homepage": "https://github.com/Blog404DE/WetterwarnungDownloader",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Dutzi",
            "email": "info@tf-network.de",
            "homepage": "https://www.tf-network.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Blog404DE/WetterwarnungDownloader/issues"
    },
    "require": {
        "php": "^7.1 <7.5",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-ftp": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ext-libxml": "*",
        "ext-curl": "*",
        "abraham/twitteroauth": "@stable"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpmd/phpmd": "^2.8",
        "friendsofphp/php-cs-fixer": "^3.0",
        "sebastian/phpcpd": "^6.0",
        "wapmorgan/php-deprecation-detector": "^2.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "blog404de\\": "botLib/blog404de"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.2.x-dev"
        }
    }
}
